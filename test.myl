section Test{{
  VAR(x): 10
  VAR(y): 3
}}

section Believe{{
    (x): 20
    (y): 9
    io.warn (x)
    io.print as u8 255
    io.print ((x) .. (y))
}}

section IfAndElse{{
    var(eta): 5
    
    if (eta) == 5 {
        io.print as txt "x e 5"
    }
    
    io.print as txt "Dopo if"
}}

section ChooseTest{{
    io.print as txt "Test Choose:"
    
    var(result): choose {
        60 => 1
        30 => 2
        10 => 3
    }
    
    io.print as txt "Risultato scelto:"
    io.print (result)
    
    var(msg): choose {
        0 => "Opzione A"
        0 => "Opzione B"
        100 => "Opzione C"
    }
    

    
    io.print as txt "Messaggio:"
    io.print (msg)

    choose {
        60 => io.warn as u8 1
        40 => io.warn as u8 2
    }
}}



program.run[
    order: {Test, Believe, IfAndElse, ChooseTest},
]
